{
    "name": "xiaoler/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["microframework","rest","router", "psr7"],
    "homepage": "http://github.com/XiaoLer/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Scholer Liu",
            "email": "scholer_l@live.com",
            "homepage": "http://0x1.im/"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/container": "^5.8",
        "illuminate/database": "^5.8",
        "monolog/monolog": "^1.17",
        "slim/http": "^0.8.0",
        "slim/psr7": "^0.3.0",
        "slim/slim": "^4.2",
        "symfony/console": "^4.2",
        "vlucas/phpdotenv": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public"
    }
}
